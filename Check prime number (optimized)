#include <iostream>
#include <cmath>
using namespace std;

bool isPrime(int n)
{
    // Base cases
    if (n <= 1) return false;  // 0 and 1 are not prime
    if (n == 2 || n == 3) return true;  // 2 and 3 are prime

    // Eliminate multiples of 2 and 3
    if (n % 2 == 0 || n % 3 == 0) return false;

    // Check divisibility by numbers of form 6k ? 1
    for (int j = 5; j * j <= n; j += 6)
    {
        if (n % j == 0 || n % (j + 2) == 0)
            return false;
    }

    return true;  // If no divisors found, number is prime
}


