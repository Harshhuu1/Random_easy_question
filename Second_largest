#include <bits/stdc++.h> 
int findSecondLargest(int n, vector<int> &arr)
{
    // Write your code here.
    if(n<2) return -1;
    int max=arr[0];
    int smax=INT_MIN;
    for(int i=0;i<n;i++){
        if(max<arr[i]){
            smax=max;
            max=arr[i];
        }
        if(arr[i]>smax && arr[i]!=max)
        smax=arr[i];
       
    }
    if(smax==INT_MIN)
    return -1;
     
    return smax;

}
