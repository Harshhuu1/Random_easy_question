#include <bits/stdc++.h> 
void sort012(int *arr, int n)
{
   //   Write your code here
   int i=0;
   int j=n-1;
   int mid=0;
   while(mid<=j){
      if(arr[mid]==0){
         swap(arr[mid],arr[i]);
         mid++;
         i++;
      }
      else if(arr[mid]==1){
         mid++;

      }
      else if(arr[mid]==2){
      swap(arr[mid],arr[j]);
   
      j--;}
      
   }
}
