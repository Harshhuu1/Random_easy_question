class Solution {
public:
    bool isPalindrome(int x) {
    long long  num=0;
    int original=x;
    long long sum=0;
    while(x>0){
        int rem=x%10;
        sum=sum*10+rem;
        x=x/10;
    }
    if(sum==original)
    return true;
    
    return false;
    }
};
// Always see the thing that stores the original number previously to check and compare later.
