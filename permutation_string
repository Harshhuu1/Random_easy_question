#include<bits/stdc++.h>
#include<unordered_map>
bool isPermutation(char input1[], char input2[]) {
    // Write your code here
    vector<int> freq(26,0);
    for(int i=0;input1[i]!='\0';i++)
    {
        freq[input1[i]-'a']++;
    }
    for(int i=0;input2[i]!='\0';i++)
    {
        freq[input2[i]-'a']--;
    }
    for(auto count:freq){
        if(count!=0) return false;
    }
    return true;
    
    

}
