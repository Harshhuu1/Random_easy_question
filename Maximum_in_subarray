#include <vector>
#include <bits/stdc++.h>
void printSubarrayMax(int *arr, int n, int k)
{
    //Write your code here
    if(k<1 ||k>n-1) return;
    vector<int> ans;
    priority_queue<pair<int,int>> maxheap;
    for(int i=0;i<n;i++){
        maxheap.push({arr[i],i});
        while(!maxheap.empty() && maxheap.top().second<=i-k){
            maxheap.pop();}
            if(i>=k-1){
                    ans.push_back(maxheap.top().first);
            }
            
        }
    
    for(auto i:ans)
    {
        cout<<i<<" ";
    }
}
