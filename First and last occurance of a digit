#include <bits/stdc++.h> 
using namespace std;

int lowerBound(vector<int> &arr, int n, int k) {
    int low = 0, high = n - 1;
    int ans = n; // default if no element >= k
    while (low <= high) {
        int mid = low + (high - low) / 2;
        if (arr[mid] >= k) {
            ans = mid; 
            high = mid - 1;
        } else {
            low = mid + 1;
        }
    }
    return ans;
}

int upperBound(vector<int> &arr, int n, int k) {
    int low = 0, high = n - 1;
    int ans = n; // default if no element > k
    while (low <= high) {
        int mid = low + (high - low) / 2;
        if (arr[mid] > k) {
            ans = mid;
            high = mid - 1;
        } else {
            low = mid + 1;
        }
    }
    return ans;
}

pair<int, int> firstAndLastPosition(vector<int>& arr, int n, int k)
{
    int first = lowerBound(arr, n, k);
    
    // if 'k' doesn't exist in array
    if (first == n || arr[first] != k)
        return {-1, -1};

    int last = upperBound(arr, n, k) - 1;

    return {first, last};
}
